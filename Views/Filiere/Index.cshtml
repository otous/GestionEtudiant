@model IEnumerable<GestionEtudiants.Models.Filiere>

@{
    ViewData["Title"] = "GetAll";
}

<h1>GetAll</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FiliereName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FiliereName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FiliereId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FiliereId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FiliereId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
